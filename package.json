    "@types/node": "^13.13.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.5",
    "karma": "^6.3.16",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-mocha": "^2.0.1",
